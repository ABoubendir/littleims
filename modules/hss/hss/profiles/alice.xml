<?xml version="1.0"?>
<IMSSubscription>
	<PrivateID>alice_private</PrivateID>
	<ServiceProfile>
		<PublicIdentity>
			<Identity>sip:alice@cipango.org</Identity>
		</PublicIdentity>
			<!--
			These IFCs are provided as samples. They are designed for invoking NGN services
			and presence servers.of NEXCOM Systems solution.
			
			<InitialFilterCriteria>
			<Priority>0</Priority>
			<TriggerPoint>
				--><!-- INVITE && terminating
				<ConditionTypeCNF>1</ConditionTypeCNF>
				<SPT>
					<ConditionNegated>0</ConditionNegated>
					<Group>0</Group>
					<Method>INVITE</Method>
				</SPT>			
				<SPT>
					<ConditionNegated>1</ConditionNegated>
					<Group>1</Group>
					<SessionCase>0</SessionCase>
				</SPT>			
			</TriggerPoint>
			<ApplicationServer>
				<ServerName>sip:ngnas@127.0.0.1:5080;lr</ServerName>
				<DefaultHandling>0</DefaultHandling>
			</ApplicationServer>
		</InitialFilterCriteria>
		
		<InitialFilterCriteria>
			<Priority>1</Priority>
			<TriggerPoint>
				--><!-- (INVITE || REGISTER) && Originating
				<ConditionTypeCNF>1</ConditionTypeCNF>
				<SPT>
					<ConditionNegated>0</ConditionNegated>
					<Group>0</Group>
					<Method>INVITE</Method>
				</SPT>	
				<SPT>
					<ConditionNegated>0</ConditionNegated>
					<Group>0</Group>
					<Method>REGISTER</Method>
				</SPT>
				<SPT>
					<ConditionNegated>0</ConditionNegated>
					<Group>1</Group>
					<SessionCase>0</SessionCase>
				</SPT>			
			</TriggerPoint>
			<ApplicationServer>
				<ServerName>sip:ngnas@127.0.0.1:5080;lr;orig</ServerName>
				<DefaultHandling>0</DefaultHandling>
			</ApplicationServer>
		</InitialFilterCriteria>		

		<InitialFilterCriteria>
			<Priority>2</Priority>
			--><!-- (SUBSCRIBE && Terminating) || (REGISTER && Originating)
			<TriggerPoint>
				<ConditionTypeCNF>0</ConditionTypeCNF>
				<SPT>
					<ConditionNegated>0</ConditionNegated>
					<Group>0</Group>
					<Method>SUBSCRIBE</Method>
				</SPT>	
				<SPT>
					<ConditionNegated>1</ConditionNegated>
					<Group>0</Group>
					<SessionCase>0</SessionCase>
				</SPT>
				<SPT>
					<ConditionNegated>0</ConditionNegated>
					<Group>1</Group>
					<Method>REGISTER</Method>
				</SPT>	
				<SPT>
					<ConditionNegated>0</ConditionNegated>
					<Group>1</Group>
					<SessionCase>0</SessionCase>
				</SPT>				
			</TriggerPoint>		
			<ApplicationServer>
				<ServerName>sip:presence@127.0.0.1:5081;lr</ServerName>
				<DefaultHandling>0</DefaultHandling>
			</ApplicationServer>
		</InitialFilterCriteria>
-->
	</ServiceProfile>	
	<ServiceProfile>
		<PublicIdentity>
			<Identity>tel:1234</Identity>
		</PublicIdentity>
	</ServiceProfile>
</IMSSubscription>
<!--
	Remember
	session case:
			0: Originated
			1: Terminated registered
			2: Terminated non registered
			
	ConditionTypeCNF:
			0: DNF Dijonctive normal form  (A && B) || D
			1: CNF Conjonctive normal form (A || B) && D				

	Default handling (used when AS could not be reached):
			0: SESSION_CONTINUED
			1: SESSION_TERMINATED
-->