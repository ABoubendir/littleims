<?xml version="1.0" encoding="UTF-8"?>
<sip-app xmlns="http://www.jcp.org/xml/ns/sipservlet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.jcp.org/xml/ns/sipservlet http://www.jcp.org/xml/ns/sipservlet/sip-app_1_1.xsd"
	version="1.1">

	<app-name>org.cipango.ims.hss</app-name>
	
	<listener>
			<listener-class>org.cipango.ims.hss.HssServlet</listener-class>
	</listener>
		
	<servlet-selection>
		<main-servlet>hss</main-servlet>
	</servlet-selection>
	
	<servlet>
		<servlet-name>hss</servlet-name>
		<servlet-class>org.cipango.ims.hss.HssServlet</servlet-class>
		<load-on-startup/>
	</servlet>
</sip-app>

